{
	"name": "slash-deduplication",
	"version": "0.1.0",
	"description": "Deduplicate multiple consecutive slashes in a current URL",
	"repository": "shinnn/slash-deduplication",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
		"pretest": "eslint .",
		"test": "node test.js"
	},
	"license": "ISC",
	"files": [
		"index.js"
	],
	"keywords": [
		"url",
		"pathname",
		"slash",
		"dedupe",
		"deduplicate",
		"history",
		"replace",
		"redirect",
		"browser",
		"client-side"
	],
	"devDependencies": {
		"@shinnn/eslint-config": "^6.8.1",
		"eslint": "^5.9.0",
		"lodash": "^4.17.11",
		"selenium-webdriver": "^4.0.0-alpha.1",
		"tape": "^4.9.1"
	},
	"eslintConfig": {
		"extends": "@shinnn",
		"overrides": [
			{
				"files": [
					"index.js"
				],
				"env": {
					"browser": true
				},
				"rules": {
					"require-unicode-regexp": "off"
				}
			}
		]
	}
